{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Quartz": "Debug"
    }
  },
  "KeyVault": {
    "VaultUri": ""
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "Authentication": {
    "RequireHttpsMetadata": true
  },
  "SchedulerSettings": {
    "StartupRecovery": {
      "Enabled": true,
      "GracePeriodMinutes": 10,
      "DelaySeconds": 0
    },
    "MissedScheduleHandling": {
      "EnableAutoFire": true,
      "MissedScheduleWindowDays": 1,
      "EnableInDevelopment": true,
      "ThrottlePerSecond": 50
    }
  }
}
